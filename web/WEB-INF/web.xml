<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServletLogin</servlet-name>
        <servlet-class>ServletLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RtaHtml</servlet-name>
        <servlet-class>RtaHtml</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RtaXml</servlet-name>
        <servlet-class>RtaXml</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RtaJson</servlet-name>
        <servlet-class>RtaJson</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RtaImage</servlet-name>
        <servlet-class>RtaImage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletLogin</servlet-name>
        <url-pattern>/servletLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RtaHtml</servlet-name>
        <url-pattern>/rtaHtml</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RtaXml</servlet-name>
        <url-pattern>/rtaXml</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RtaJson</servlet-name>
        <url-pattern>/rtaJson</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RtaImage</servlet-name>
        <url-pattern>/rtaImage</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
